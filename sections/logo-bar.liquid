<div class="page-width">
  {% if section.settings.title != blank %}
    <div class="section-header text-center">
      <h2 id="title_esc">{{ section.settings.title | escape }}</h2>
    </div>
  {% endif %}
  <div class="custom-display-xl">
    {% if section.blocks.size > 0 %}
      <table>
        <tr>
      {% for block in section.blocks %}
          {% if forloop.first == true %}
          <tr>
            <th class="border_none" rowspan="2">
              <div>
                {% if block.settings.link != blank %}
                  <a href="{{ block.settings.link }}">
                {% endif %}
                {% if block.settings.image != blank %}
                  <img style="padding-right: 5px; height: 374px;width: 340px;" class="custom-big-image"  src="{{block.settings.image | img_url: '1000x1000'}}" alt="{{block.settings.image.alt}}">
                {% else %}
                  {{ 'logo' | placeholder_svg_tag: 'placeholder-svg' }}
                {% endif %}
                {% if block.settings.link != blank %}
                  </a>
                {% endif %}
              </div>
            </th>
          {% elsif forloop.index == 5 %}
            <th class="border_none">
              <li class="logo-bar__item{% if section.settings.title_enable %} logo-bar__item--large{% endif %}" {{ block.shopify_attributes }}>
                {% if block.settings.link != blank %}
                  <a href="{{ block.settings.link }}" class="logo-bar__link">
                {% endif %}
                {% if block.settings.image != blank %}
                  {{ block.settings.image | img_url: '1000x1000', scale: 2 | img_tag: block.settings.image.alt, 'logo-bar__image' }}
                {% else %}
                  {{ 'logo' | placeholder_svg_tag: 'placeholder-svg' }}
                {% endif %}
                {% if block.settings.link != blank %}
                  </a>
                {% endif %}
              </li>
            </th>
          </tr>
          {% elsif forloop.index == 6 %}
          <tr>
            <th class="border_none">
              <li class="logo-bar__item{% if section.settings.title_enable %} logo-bar__item--large{% endif %}" {{ block.shopify_attributes }}>
                {% if block.settings.link != blank %}
                  <a href="{{ block.settings.link }}" class="logo-bar__link">
                {% endif %}
                {% if block.settings.image != blank %}
                  {{ block.settings.image | img_url: '1000x1000', scale: 2 | img_tag: block.settings.image.alt, 'logo-bar__image' }}
                {% else %}
                  {{ 'logo' | placeholder_svg_tag: 'placeholder-svg' }}
                {% endif %}
                {% if block.settings.link != blank %}
                  </a>
                {% endif %}
              </li>
            </th>
           {% elsif forloop.index == 9 %}
            <th class="border_none">
              <li class="logo-bar__item{% if section.settings.title_enable %} logo-bar__item--large{% endif %}" {{ block.shopify_attributes }}>
                {% if block.settings.link != blank %}
                  <a href="{{ block.settings.link }}" class="logo-bar__link">
                {% endif %}
                {% if block.settings.image != blank %}
                  {{ block.settings.image | img_url: '1000x1000', scale: 2 | img_tag: block.settings.image.alt, 'logo-bar__image' }}
                {% else %}
                  {{ 'logo' | placeholder_svg_tag: 'placeholder-svg' }}
                {% endif %}
                {% if block.settings.link != blank %}
                  </a>
                {% endif %}
              </li>
            </th>
          </tr>
          {% elsif forloop.index > 9 %}
        
        
          {% else %}
          <th class="border_none">
            <li class="logo-bar__item{% if section.settings.title_enable %} logo-bar__item--large{% endif %}" {{ block.shopify_attributes }}>
              {% if block.settings.link != blank %}
                <a href="{{ block.settings.link }}" class="logo-bar__link">
                  {% endif %}
                  {% if block.settings.image != blank %}
                  {{ block.settings.image | img_url: '1000x1000', scale: 2 | img_tag: block.settings.image.alt, 'logo-bar__image' }}
                  {% else %}
                  {{ 'logo' | placeholder_svg_tag: 'placeholder-svg' }}
                  {% endif %}
                  {% if block.settings.link != blank %}
                </a>
              {% endif %}
            </li>
          </th>
      	{% endif %}
      {% endfor %}
       </table>
  {% endif %}
