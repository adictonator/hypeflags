.upper_dots,
.bottom_dots {
	width: 100%;
	z-index: 3;
	position: absolute;
	height: 100%;
	pointer-events: none;
}
.upper_dots:after,
.upper_dots:before,
.bottom_dots:after,
.bottom_dots:before {
	display: block;
	content: '';
	width: 1.91%;
	height: 3.18%;
	background: #d8d8d8;
	border-radius: 50%;
	position: absolute;
	top: 5px;
	left: 5px;
	border: 2px solid #d9cc8f;
}
.upper_dots:after,
.bottom_dots:after {
	right: 5px;
	top: 5px;
	left: auto;
}
.bottom_dots:after,
.bottom_dots:before {
	bottom: 5px;
	top: auto;
}
.bottom_dots {
	bottom: 0;
}

.tweet_theme_box {
	margin: 20px 25px 0 0;
}
.t_theme_choice {
	display: flex;
	justify-content: space-between;
}
.t_theme_choice label {
	flex: 1;
}
.t_theme_choice label:not(:last-of-type) {
	margin-right: 20px;
}
.t_theme_choice label:hover span:after {
	box-shadow: 0 0 0 10px #0000001a;
}
.t_theme_choice label input {
	opacity: 0;
	position: absolute;
}
.t_theme_choice label input:checked + span {
	border-color: #2aa3ef;
}
.t_theme_choice label input:checked + span:before {
	background-color: #1da1f2;
	background-image: url(//cdn.shopify.com/s/files/1/0254/3868/3184/t/67/assets/twitter-tick.svg?v=74796769104245093101654624337);
	border-color: #1da1f2;
}
.t_theme_choice label span {
	display: flex;
	align-items: center;
	width: 100%;
	padding: 12px 20px;
	background: transparent;
	border-radius: 4px;
	position: relative;
	cursor: pointer;
	border: 2px solid #898f94;
	font-weight: 700;
	font-size: 17px;
}
.t_theme_choice label span:before {
	content: '';
	display: flex;
	height: 20px;
	width: 20px;
	min-width: 20px;
	border-radius: 50px;
	background: transparent;
	border: 2px solid #898f94;
	align-items: center;
	justify-content: center;
	transition: background 0.2s linear, box-shadow 0.2s linear;
	margin-right: 15px;
}
.t_theme_choice label span:before svg path {
	fill: #fff;
}
@media (max-width: 767px) {
	.t_theme_choice label span {
		font-size: 12px;
		padding: 8px 18px;
		margin-right: 10px;
	}
}
.t_theme_choice label.t_dark_mode:hover span:after {
	box-shadow: 0 0 0 10px #ffffff1a;
}
.t_theme_choice label.t_dark_mode input:checked + span {
	border-color: #2aa3ef;
}
.t_theme_choice label.t_dark_mode span {
	background: #15202b;
	border-color: #898f94;
	color: #fff;
}
