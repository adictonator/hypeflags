.tweet-flag-wrap {
	display: flex;
	justify-content: center;
	align-items: center;
	padding: 28px;
	background: #ebebeb;
	width: 100%;
	flex-direction: column;
    height: 100%;
    border-radius: 5px;
}

.tweet_time {
	white-space: nowrap;
}
.twitter_wrapper {
	font-size: 15px;
	line-height: 1.3125;
	position: relative;
	font-weight: 400;
	margin: 0 auto;
	padding: 15px;
	box-sizing: border-box;
	font-family: Helvetica Neue, sans-serif;
}
select#theme_color {
	margin: 12px 0 0;
	min-width: 150px;
	outline: none;
	border: 1px solid #ccc;
}
.edit-tweet-popup a.t_popup_toggle {
	display: none;
}
.tweet_content {
	color: #666;
	line-height: 1.3125;
}
.tweet_content p,
#twitterText,
#editable_text {
	color: #000;
	margin: 15px 0;
	font-size: 23px;
	letter-spacing: normal !important;
	white-space: pre-wrap;
	font-family: Helvetica Neue, sans-serif;
	overflow: visible;
	min-width: 461px;
	width: 461px;
}
.twitt_img {
	border-radius: 20px;
	overflow: hidden;
	margin: 0 0 10px;
}
.twitt_img img {
	width: 100%;
	height: 300px;
	object-fit: cover;
	display: block;
}
.twitt_video {
	border-radius: 20px;
	overflow: hidden;
	margin: 0 0 10px;
}
.tweet_video_img {
	position: relative;
	background: #000;
}
.tweet_video_img img {
	width: 100%;
	height: 300px;
	object-fit: cover;
	display: block;
}
.current_views {
	position: absolute;
	bottom: 15px;
	left: 21px;
	color: #ffffffb3;
}
.play_tweet_video {
	background: transparent;
	border: 0;
	padding: 0;
	outline: none;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
	cursor: pointer;
	font-size: 90px;
	color: #fff;
}
.tweet_time {
	color: #5b7083;
	display: flex;
	align-items: center;
}
.tweet_reactions {
	display: flex;
	padding: 15px 5px;
	border-top: 1px solid #ebeef0;
	margin-top: 15px;
}
.tweet_reactions > div {
	margin-right: 20px;
}
.tweet_reactions > div strong {
	color: #000;
}
.tweet_reactions > div span {
	font-weight: 400;
	color: #5b7083;
	margin-left: 6px;
}
.tweet_action_count {
	padding: 12px 0 0;
	border-top: 1px solid #ebeef0;
	margin: 0;
}
.tweet_action_count ul {
	list-style: none;
	padding: 0;
	margin: 0;
	display: flex;
	justify-content: space-around;
	line-height: 1;
}
.tweet_action_count ul li {
	text-decoration: none;
	display: flex;
	align-items: center;
}
.tweet_action_count ul li a {
	color: #5b7083;
	text-decoration: none;
	display: flex;
	align-items: center;
}
.tweet_action_count ul li a i {
	font-size: 22px;
}
.tweet_action_count ul li svg {
	width: 20px;
}
.tweet_action_count ul li svg path {
	fill: #5b7083;
}
.edit-tweet-popup {
	position: fixed;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	background: rgba(0, 0, 0, 0.6);
	z-index: 99999;
	display: none;
	overflow: auto;
}
.edit-tweet-popup.opened {
	display: block;
}
.edit-tweet-popup_inner {
	height: auto;
	max-width: 800px;
	width: 95%;
	margin: 20px auto;
	box-shadow: 0 4px 12px #0003;
	background: #fff;
	border-radius: 6px;
	-webkit-border-radius: 6px;
	overflow: hidden;
	padding: 25px 30px;
}
.t_popup_save {
	text-align: center;
	display: flex;
	align-items: center;
	justify-content: space-between;
	height: auto;
	background: #f3f3f3;
	margin: 30px -30px -25px;
	padding: 20px 30px;
	flex-direction: row-reverse;
}
.t_p_save,
.close_t_popup {
	background: #d5003a;
	color: #fff;
	border: 0;
	padding: 15px;
	line-height: 1;
	min-width: 130px;
	border-radius: 6px;
	text-transform: uppercase;
	cursor: pointer;
	font-weight: 700;
	font-size: 15px;
}
.close_t_popup {
	background: transparent;
	margin: 0 0 0 10px;
	color: #000;
	letter-spacing: 2px;
	border: 1px solid #000;
}
.t_p_save:hover,
.close_t_popup:hover {
	background: #000;
	color: #fff !important;
	border-color: #000;
}
.twitt_p_head {
	position: relative;
	text-align: center;
	padding: 0 0 25px;
	margin: 0 0 30px;
	border-bottom: 1px solid #000;
}
.twitt_p_head a {
	position: absolute;
	right: 0;
	top: 0;
	font-weight: 400;
	font-size: 22px;
	color: #d5003a;
}
.twitt_p_head h3 {
	margin: 0;
	font-size: 22px;
	color: #606163;
}
.edit-tweet-popup_inner .tweet_content .editable_text {
	border: 0;
	width: 100%;
	padding: 0;
	resize: none;
	background: transparent;
	outline: none;
	font-family: Helvetica Neue, sans-serif;
}
